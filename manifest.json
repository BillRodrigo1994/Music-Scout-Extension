{
  "manifest_version": 3,
  "name": "Music Scout",
  "version": "0.1",
  "description": "Spotify Music Recommendation and Fast Access",
  "icons": {
    "16": "icons/logo16.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  },
  "permissions": ["contextMenus", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Go To Music Scout Web"
  }
}
